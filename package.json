{
  "name": "typescript-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"tsc -w -p src\" \"nodemon dist/src/index.js\"",
    "serve": "concurrently --kill-others \"tsc -p src\" \"node dist/index.js\"",
    "debug": "concurrently --kill-others \"tsc -w -p src\" \"nodemon --inspect dist/index.js\"",
    "test:watch": "mocha -r ts-node/register -w ./spec/**/*.spec.ts --watch-extensions ts",
    "test": "mocha -r ts-node/register -w ./spec/**/*.spec.ts"
  },
  "dependencies": {
    "@types/express": "^4.0.36",
    "@types/lodash": "^4.14.68",
    "@types/mongoose": "^4.7.18",
    "@types/node": "^8.0.9",
    "body-parser": "~1.13.2",
    "concurrently": "^3.5.0",
    "cookie-parser": "~1.3.5",
    "cross-env": "^5.1.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "glob": "^5.0.14",
    "jade": "~1.11.0",
    "lodash": "^3.10.1",
    "mongoose": "^4.11.1",
    "morgan": "~1.6.1",
    "nodemon": "^1.17.4",
    "passport": "^0.3.2",
    "q": "^1.4.1",
    "serve-favicon": "~2.3.0",
    "tsc": "^1.20150623.0",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/sinon": "^4.3.3",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "node-mocks-http": "^1.7.0",
    "sinon": "^5.0.7",
    "ts-node": "^6.0.3"
  }
}
